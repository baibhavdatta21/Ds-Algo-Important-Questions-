Brute force
TC->O(N*N)
SC->O(N*N)
/*void rotate(vector<vector<int>>& matrix)
    {
        int n=matrix.size();
        vector<vector<int>> res;
        res=matrix;
        for(int i=0;i<n;i++)
        {
            for(int j=0;j<n;j++)
